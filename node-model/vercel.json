{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "src/index.ts"
        }
    ],
    "scripts": {
        "build": "npm run build:ts",
        "build:ts": "tsc",
        "start": "node dist/index.js"
    }
}